<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.HibernateSpecialitiesEntity" table="hibernate_specialities" schema="public"
           catalog="hibernate1">
        <id name="id">
            <column name="id" sql-type="integer"/>
            <generator class="identity"/>
        </id>
        <property name="speciality">
            <column name="speciality" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>